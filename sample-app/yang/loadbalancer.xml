<?xml version="1.0"?>
<LoadBalancerConfig xmlns="urn:meetup:loadbalancer">
    <id>1</id>
    <PortGroups>
        <id>1</id>
        <PortGroup>
            <id>testPortGroup</id>
            <port>8081</port>
            <port>8082</port>
        </PortGroup>
    </PortGroups>
    <Rules>
        <id>1</id>
        <Rule>
            <id>cool_rule_1</id>
            <from>127.0.0.*</from>
            <to>
                <group>testPortGroup</group>
            </to>
        </Rule>
        <Rule>
            <id>cool_rule_2</id>
            <from>127.0.1.*</from>
            <to>
                <port>8080</port>
            </to>
        </Rule>
    </Rules>
</LoadBalancerConfig>
